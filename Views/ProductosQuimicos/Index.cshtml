@model IEnumerable<ProyectoCore.Models.ProductoQuimico>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Costo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdExportacionNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTrabajadorNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Costo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdExportacionNavigation.IdExportacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTrabajadorNavigation.IdTrabajador)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdProductoQuimico">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdProductoQuimico">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdProductoQuimico">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
